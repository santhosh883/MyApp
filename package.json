{
  "name": "DataDeskReact",
  "version": "1.0.0",
  "description": "DataDesk React",
  "author": "Santosh Jayabalan",
  "license": "ISS",
  "scripts": {
    "watch": "webpack -d --watch --progress",
    "build": "webpack -p",
    "start": "webpack-dev-server --open --progress",
    "prettify": "prettier src/** --write",
    "test": "mocha --compilers js:babel-register --require babel-polyfill --recursive spec --require spec/spec.js",
    "dtest": "mocha --inspect --inspect-brk --compilers js:babel-register --require babel-polyfill --recursive spec --require spec/spec.js",
    "htest": "mocha --help",
    "postinstall": "node -e \"var s='../src/components',d='node_modules/iss',fs=require('fs');fs.exists(d,function(e){e||fs.symlinkSync(s,d,'dir')});\" # see https://gist.github.com/branneman/8048520",
    "precommit": "pretty-quick --staged"
  },
  "dependencies": {
    "alt": "^0.18.6",
    "alt-container": "^1.1.1",
    "angular": "^1.6.1",
    "immutable": "^3.8.2",
    "material-ui": "^0.20.0",
    "moment": "^2.22.0",
    "npm": "^5.7.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-google-charts": "^1.5.7",
    "react-moment": "^0.7.0",
    "react-tippy": "^1.2.2",
    "underscore": "^1.9.0",
    "url-polyfill": "^1.0.13",
    "uuid": "^3.2.1",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.22.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "chai": "^4.1.2",
    "css-loader": "^0.28.9",
    "file-loader": "^1.1.6",
    "json-loader": "^0.5.7",
    "mocha": "^5.0.4",
    "prettier": "^1.11.1",
    "pretty-quick": "^1.4.1",
    "style-loader": "^0.20.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.11.0",
    "webpack-cli": "2.0.9",
    "webpack-dev-server": "2.11.1",
    "webpack-node-externals": "^1.6.0"
  }
